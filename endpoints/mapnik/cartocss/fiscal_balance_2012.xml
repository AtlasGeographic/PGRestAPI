<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map>

<Style name="fiscalbalance" filter-mode="first" >
  <Rule>
    <Filter>([year] = 2012) and ([fisdef] = -999)</Filter>
    <LineSymbolizer stroke="#6e6e6e" stroke-width="0.5" />
    <PolygonSymbolizer fill="#e1e1e1" fill-opacity="1" />
  </Rule>
  <Rule>
    <Filter>([year] = 2012) and ([fisdef] &lt;= -6)</Filter>
    <LineSymbolizer stroke="#6e6e6e" stroke-width="0.5" />
    <PolygonSymbolizer fill="#fa5600" fill-opacity="1" />
  </Rule>
  <Rule>
    <Filter>([year] = 2012) and ([fisdef] &lt;= -4)</Filter>
    <LineSymbolizer stroke="#6e6e6e" stroke-width="0.5" />
    <PolygonSymbolizer fill="#fd8a00" fill-opacity="1" />
  </Rule>
  <Rule>
    <Filter>([year] = 2012) and ([fisdef] &lt;= -2)</Filter>
    <LineSymbolizer stroke="#6e6e6e" stroke-width="0.5" />
    <PolygonSymbolizer fill="#fd8a00" fill-opacity="1" />
  </Rule>
  <Rule>
    <Filter>([year] = 2012) and ([fisdef] &lt;= 0)</Filter>
    <LineSymbolizer stroke="#6e6e6e" stroke-width="0.5" />
    <PolygonSymbolizer fill="#f5f500" fill-opacity="1" />
  </Rule>
  <Rule>
    <Filter>([year] = 2012) and ([fisdef] &lt;= 2)</Filter>
    <LineSymbolizer stroke="#6e6e6e" stroke-width="0.5" />
    <PolygonSymbolizer fill="#c6f700" fill-opacity="1" />
  </Rule>
  <Rule>
    <Filter>([year] = 2012) and ([fisdef] &lt;= 4)</Filter>
    <LineSymbolizer stroke="#6e6e6e" stroke-width="0.5" />
    <PolygonSymbolizer fill="#94f700" fill-opacity="1" />
  </Rule>
  <Rule>
    <Filter>([year] = 2012) and ([fisdef] &lt;= 6)</Filter>
    <LineSymbolizer stroke="#6e6e6e" stroke-width="0.5" />
    <PolygonSymbolizer fill="#5ff700" fill-opacity="1" />
  </Rule>
  <Rule>
    <Filter>([year] = 2012) and ([fisdef] &gt; 6)</Filter>
    <LineSymbolizer stroke="#6e6e6e" stroke-width="0.5" />
    <PolygonSymbolizer fill="#00f500" fill-opacity="1" />
  </Rule>
  <Rule>
    <Filter>([year] = 2012)</Filter>
    <LineSymbolizer stroke="#6e6e6e" stroke-width="0.5" />
    <PolygonSymbolizer fill-opacity="1" fill="#aaee88" />
  </Rule>
</Style>

</Map>