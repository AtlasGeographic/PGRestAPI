<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<Map minimum-version="2.0.0">

<Style name="rast_default" filter-mode="first">
  <Rule>
    <RasterSymbolizer opacity="1" scaling="bilinear">
      <RasterColorizer default-mode="linear" default-color="white" epsilon="0.001">
        <stop color="blue"        value = "-1000"  />
        <stop color="red"         value = "-500"   />
        <stop color="yellow"      value = "0"      />
        <stop color="red"         value = "10"     />
        <stop color="green"       value = "15"     />
        <stop color="black"       value = "17"     />
        <stop color="indigo"      value = "100"    />
      </RasterColorizer>
    </RasterSymbolizer>
  </Rule>
</Style>

</Map>